#include<iostream>

using namespace std;

int main(){
   float p1, p2, media = 0;
   
   cin >> p1 >> p2;
   while(p1 < 0 || p1 > 10 || p2 < 0 || p2 > 10){
        if(p2 >= 0 && p2 <= 10 &&p1 < 0 || p1 > 10){
            cout << "nota invalida" << endl;
            cin >> p1;
        } 
        else if(p1 >= 0 && p1 <= 10 && p2 < 0 || p2 > 10){
            cout << "nota invalida" << endl;
            cin >> p2;
        }
        else { 
            cout << "nota invalida" << endl;
            cin >> p1 >> p2;
        } 
   }
   media = (p1+p2)/2;
   cout << "media = " << media << endl;
}
